#use "topfind";;
#require "atdgen";;
#load "category.cmo";;
#load "results_j.cmo";;

let read_file name =
	let ic = open_in name in
	let length = in_channel_length ic in
	really_input_string ic length;;

let race_contents = read_file "ages.results.json";;

let race = Results_j.race_of_string race_contents;;

let sprint_contents = read_file "ages.sprints.json";;

let sprints = Results_j.sprints_of_string sprint_contents;;
